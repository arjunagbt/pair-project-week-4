<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Project List</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="/style.css" rel="stylesheet" type="text/css">
    <script src="main.js"></script>
</head>
<body>
    <h1>Project Lists</h1>
    <a href="/">Home</a>
    <table>
        <tr>
            <th>id</th>
            <th>Project Name</th>
            <th>Project Description</th>
            <th>Funding Progress</th>
            <th>Amount Needed</th>
            <th>Started By</th>
        </tr>
        <% projects.forEach(function(data){ %>
            <tr>
                <td><%=data.id%></td>
                <td><a href="/projects/details/<%=data.id%>"><%=data.name%></a></td>
                <td><%=data.description%></td>
                <td><%=data.getPercent()%></td>
                <td><%=data.nominal_needed%></td>
                <td><%=data.User.full_name%></td>
            </tr>
          <% }); %>
    </table>
    <a href="/projects/add">Start your own project!</a>
</body>
</html>